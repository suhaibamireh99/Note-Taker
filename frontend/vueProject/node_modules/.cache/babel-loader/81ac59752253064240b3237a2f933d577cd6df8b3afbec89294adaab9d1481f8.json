  autocomplete=\"on\"\n      class=\"create-note-form-create-note-form\"\n    >\n      <h2 id=\"createNoteHeader\" class=\"create-note-form-create-note-title\">\n        <span class=\"create-note-form-text\">Create Note</span>\n        <br />\n      </h2>\n      <input\n        type=\"text\"\n        id=\"inputNoteTitle\"\n        name=\"noteTitleInput\"\n        required\n        maxlength=\"32\"\n        :placeholder=\"TitleInput_placeholder\"\n        autocomplete=\"on\"\n        v-model=\"note.title\"\n        class=\"create-note-form-title-input input\"\n      />\n      <input\n        type=\"text\"\n        id=\"inputLabelName\"\n        name=\"inputLabelName\"\n        maxlength=\"32\"\n        :placeholder=\"LabelNameInput_placeholder\"\n        autocomplete=\"on\"\n        v-model=\"note.labelName\"\n        class=\"create-note-form-label-name-input input\"\n      />\n      <label\n        id=\"noteTitleHeader\"\n        for=\"inputNoteTitle\"\n        class=\"create-note-form-note-titlet-header\"\n      >\n        <span>Enter Note Title</span>\n        <br />\n      </label>\n      <textarea\n        id=\"noteContentInput\"\n        name=\"noteContentInput\"\n        maxlength=\"300\"\n        :placeholder=\"NoteContentTextArea_placeholder\"\n        v-model=\"note.content\"\n        class=\"create-note-form-note-content-text-area textarea\"\n      ></textarea>\n      <label\n        id=\"noteContentHeader\"\n        for=\"noteContentInput\"\n        class=\"create-note-form-note-content-header\"\n      >\n        <span>Enter Note Content</span>\n        <br />\n      </label>\n      <label\n        id=\"LabelnameHeader\"\n        for=\"inputLabelName\"\n        class=\"create-note-form-label-name-header\"\n      >\n        <span>Enter Label Name</span>\n        <br />\n      </label>\n      <button @click=\"saveNote\" class=\"btn btn-success\">Submit</button>\n    \n      <!-- <router-link\n        to=\"/home\"\n        id=\"cancelBtn\"\n        name=\"cancelBtn\"\n        class=\"create-note-form-save-btn1 button\"\n      >\n        {{ cancelBtn }}\n      </router-link> -->\n    </form>\n  </div>\n</template>\n\n<script>\nimport NoteDataService from \"../services/NoteDataService\";\n\nexport default {\n  name: 'CreateNoteForm',\n  props: {\n    LabelNameInput_placeholder: {\n      type: String,\n      default: 'Label-Name',\n    },\n    cancelBtn: {\n      type: String,\n      default: 'Cancel',\n    },\n    saveBtn: {\n      type: String,\n      default: 'Save',\n    },\n    rootClassName: String,\n    NoteContentTextArea_placeholder: {\n      type: String,\n      default: 'note-content',\n    },\n    TitleInput_placeholder: {\n      type: String,\n      default: 'Note-Title',\n    },\n  },\n  data() {\n    return {\n      note: {\n        id: null,\n        title: \"\",\n        content: \"\",\n        labelName: \"\"\n      },\n      submitted: false\n    };\n  },\n  methods: {\n    saveNote() {\n      var data = {\n        title: this.note.title,\n        content: this.note.content,\n        labelName: this.note.labelName\n      };\n\n      NoteDataService.create(data).then(response => {\n        this.note.id = response.data.id;\n        console.log(response.data);\n        this.submitted = true;\n      })\n      .catch(e => {\n        console.log(e);\n      });\n    },\n\n    newNote(){\n      this.submitted = false;\n      this.note = {};\n    }\n  }\n};\n</script>\n\n<style scoped>\n.create-note-form-container {\n  display: flex;\n  position: relative;\n}\n.create-note-form-create-note-form {\n  width: 645px;\n  height: 426px;\n  display: flex;\n  position: relative;\n  background-color: #527285;\n}\n.create-note-form-create-note-title {\n  top: 18px;\n  left: 35px;\n  width: 228px;\n  height: 58px;\n  position: absolute;\n}\n.create-note-form-text {\n  color: #ffffff;\n}\n.create-note-form-title-input {\n  top: 99px;\n  left: 16px;\n  width: 285px;\n  height: 40px;\n  position: absolute;\n  padding-top: 0px